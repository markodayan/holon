{
  "name": "urs-backend",
  "version": "1.0.0",
  "description": "URS backend subsystem consisting of scraper and core services, to be hosted in a Docker container",
  "main": "main.js",
  "scripts": {
    "scraper:dev": "pm2 start dev.config.js --only scraper",
    "scraper:core": "pm2 start dev.config.js --only core",
    "scraper:dev:build-image": "docker build -t dev/scraper -f src/services/scraper/Dockerfile.dev .",
    "scraper:dev:create-container": "docker run -d -p 5000:5000 --name scraper dev/scraper"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "noob-ethereum": "^3.0.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.1",
    "typeorm": "^0.3.11",
    "typescript": "^4.9.4",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.17",
    "@types/ws": "^8.5.3"
  }
}
